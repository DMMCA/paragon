def main():
    print("hello world: "+str(env.rand()))
    print("time: "+str(env.time()))
    print("uid: "+env.uid())
    print("user: "+env.user())

    f, err = assets.openFile("tst/file")
    assert.noError(err)

    print("filename: "+file.name(f))
    content, err = file.content(f)
    assert.noError(err)
    print("file content: "+str(content))
